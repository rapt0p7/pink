{
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 6,
		"ecmaFeatures": {
			"impliedStrict": true
		}
	},
	"env": {
		"es6":     true,
		"browser": true,
		"node":    true
	},
	"plugins": ["react", "import"],
	"extends": ["eslint:recommended", "plugin:react/recommended", "airbnb"],
	"rules": {
		"comma-dangle": ["error", "never"]
	}
}
